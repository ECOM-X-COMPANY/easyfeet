            "spec_show_empty_metafields": false,
            "spec_empty_field_text": "-",
            "tab_1_title": "",
            "tab_1_text": "",
            "tab_1_page": "",
            "tab_2_title": "",
            "tab_2_text": "",
            "tab_2_page": "",
            "tab_3_title": "",
            "tab_3_text": "",
            "tab_3_page": ""
